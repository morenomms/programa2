#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main()

{
int suma,n;
float i;
cout<<"ingrese el valor n:";
cin>>n;
suma=0;
for(i=1;i<=n;i++)
suma=suma+i*i;
cout<<"la suma de los cuadrados de los"<<n<<"primeros numeros enteros es:"<<suma;
getch();

}

